    "\n",
    "def sigmoid(x):\n",
    "    return 1 / (1 + math.exp(-x))\n",
    "\n",
    "def func(x1, x2):\n",
    "    y = 4 * x1 + 5 * x2 -9\n",
    "    print(sigmoid(y))"
